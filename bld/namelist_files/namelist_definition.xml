<entry id="stream_year_last_aero" type="integer"  category="cice" group="ice_prescaero_nml" valid_values="">
Last year in stream to use
Default: 1
</entry>

<entry id="model_year_align_aero" type="integer"  category="cice" group="ice_prescaero_nml" valid_values="">
